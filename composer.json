{
    "name": "fsi/form-extensions-bundle",
    "type": "symfony-bundle",
    "description": "FSi extensions for Symfony form component.",
    "keywords": ["fsi", "symfony", "form", "extensions", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "twig/twig": "^3.3",
        "symfony/asset": "^5.4|^6.0|^7.0",
        "symfony/options-resolver": "^5.4|^6.0|^7.0",
        "symfony/framework-bundle" : "^5.4|^6.0|^7.0",
        "symfony/twig-bundle": "^5.4|^6.0|^7.0",
        "symfony/form" : "^5.4|^6.0|^7.0",
        "symfony/validator": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "beberlei/assert": "^3.3",
        "behat/behat": "^3.16.1",
        "behat/mink": "^1.9",
        "behat/mink-selenium2-driver": "^1.4",
        "friends-of-behat/mink-browserkit-driver": "^1.6.1",
        "friends-of-behat/mink-debug-extension": "^2.1",
        "friends-of-behat/mink-extension": "^2.6",
        "friends-of-behat/symfony-extension": "^2.2",
        "phpspec/prophecy": "^1.15",
        "phpspec/phpspec": "^7.2|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-beberlei-assert": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "sensiolabs/behat-page-object-extension": "^2.3",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/console": "^5.4|^6.0|^7.0",
        "symfony/css-selector": "^5.4|^6.0|^7.0",
        "symfony/debug-bundle": "^5.4|^6.0|^7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "symfony/proxy-manager-bridge": "^5.4|^6.0",
        "symfony/routing": "^5.4|^6.0|^7.0",
        "symfony/var-dumper": "^5.4|^6.0|^7.0",
        "symfony/phpunit-bridge": "^6.3|^7.0"
    },
    "conflict": {
        "symfony/property-info": ">=8.0"
    },
    "config": {
        "bin-dir": "vendor/bin",
        "allow-plugins": {
            "ocramius/package-versions": true
        }
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\FormExtensionsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "FSi\\FixturesBundle\\": "features/fixtures/project/src/" }
    },
    "scripts": {
        "console": "features/fixtures/project/bin/console",
        "cache-clear": "features/fixtures/project/bin/console cache:clear",
        "assets": "features/fixtures/project/bin/console assets:install features/fixtures/project/public --relative --symlink"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "2.4": "2.4-dev",
            "2.3": "2.3-dev",
            "2.2": "2.2-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
