{
    "name": "fsi/form-extensions-bundle",
    "type": "symfony-bundle",
    "description": "FSi extensions for Symfony2 form component.",
    "keywords": ["fsi", "symfony", "form", "extensions", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "twig/twig": "^1.2",
        "symfony/options-resolver": "^3.0",
        "symfony/framework-bundle" : "^3.0",
        "symfony/twig-bundle": "^3.0",
        "symfony/form" : "^3.0",
        "symfony/validator": "^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.0",
        "behat/symfony2-extension": "^2.0",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.2",
        "bossa/phpspec2-expect": "^1.0",
        "phpspec/phpspec": "^2.2",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "symfony/console": "^3.0",
        "symfony/css-selector": "^3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\FormExtensionsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "1.1": "1.1-dev",
            "dev-master": "2.0-dev"
        }
    }
}
