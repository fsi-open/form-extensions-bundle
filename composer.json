{
    "name": "fsi/form-extensions-bundle",
    "type": "symfony-bundle",
    "description": "FSi extensions for Symfony form component.",
    "keywords": ["fsi", "symfony", "form", "extensions", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "twig/twig": "^2.11",
        "symfony/options-resolver": "^3.4|^4.0|^5.0",
        "symfony/framework-bundle" : "^3.4|^4.0|^5.0",
        "symfony/twig-bundle": "^3.4|^4.0|^5.0",
        "symfony/form" : "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/symfony2-extension": "^2.1",
        "behat/mink-extension": "^2.0",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-selenium2-driver": "^1.3",
        "doctrine/annotations": "^1.2",
        "phpspec/phpspec": "^4.0|^5.0|^6.0",
        "phpspec/prophecy": "^1.7",
        "sensiolabs/behat-page-object-extension": "^2.0",
        "symfony/asset": "^3.4|^4.4|^5.0",
        "symfony/console": "^3.4|^4.4|^5.0",
        "symfony/css-selector": "^3.4|^4.4|^5.0",
        "symfony/http-foundation": "^3.4|^4.4|^5.0",
        "symfony/templating": "^3.4|^4.4|^5.0",
        "symfony/var-dumper": "^3.4|^4.4|^5.0",
        "symfony/phpunit-bridge": "^5.0",
        "beberlei/assert": "^3.3"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\FormExtensionsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "FSi\\FixturesBundle\\": "features/fixtures/project/src/FSi/FixturesBundle" },
        "classmap": ["features/fixtures/project/app/AppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "1.1": "1.1-dev"
        }
    }
}
