{
    "name": "fsi/form-extensions-bundle",
    "type": "symfony-bundle",
    "description": "FSi extensions for Symfony form component.",
    "keywords": ["fsi", "symfony", "form", "extensions", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "twig/twig": "^2.14|^3.3",
        "symfony/asset": "^4.4|^5.0",
        "symfony/options-resolver": "^4.4|^5.0",
        "symfony/framework-bundle" : "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/form" : "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0"
    },
    "require-dev": {
        "beberlei/assert": "^3.3",
        "behat/behat": "^3.3",
        "behat/mink-extension": "^2.3",
        "behat/mink-selenium2-driver": "^1.4",
        "friends-of-behat/symfony-extension": "^2.2",
        "friends-of-behat/mink-browserkit-driver": "^1.5",
        "phpspec/phpspec": "^7.1",
        "phpstan/phpstan": "^0.12.46",
        "phpstan/phpstan-beberlei-assert": "^0.12.3",
        "phpstan/phpstan-doctrine": "^0.12.20",
        "sensiolabs/behat-page-object-extension": "^2.3",
        "symfony/console": "^4.4|^5.0",
        "symfony/css-selector": "^4.4|^5.0",
        "symfony/debug-bundle": "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0",
        "symfony/var-dumper": "^4.4|^5.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\FormExtensionsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "FSi\\FixturesBundle\\": "features/fixtures/project/src/" }
    },
    "scripts": {
        "console": "features/fixtures/project/bin/console",
        "cache-clear": "features/fixtures/project/bin/console cache:clear",
        "assets": "features/fixtures/project/bin/console assets:install features/fixtures/project/public --relative --symlink"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "2.1": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev"
        }
    }
}
